<?xml version="1.0" encoding="UTF-8"?>
<!-- <!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd"> -->
<suite name="DemoQASuite" 
		verbose="2"
       	annotations="JDK" 
       	configfailurepolicy="continue"
		junit="false" 
		skipfailedinvocationcounts="true" 
		preserve-order="true"
		data-provider-thread-count="2" 
		allow-return-values="true">

	<!-- Listeners -->
	<listeners>
		<listener class-name="com.listeners.TestListener" />
	</listeners>

	<!-- Method Selectors <method-selectors> <method-selector> <selector-class 
		name="com.example.selectors.CustomMethodSelector"/> <priority>0</priority> 
		</method-selector> </method-selectors> -->

	<test name="HomePageTests" preserve-order="true" verbose="2">
		<groups>
			<run>
				<include name="regression" />
				<include name="config" />
				<exclude name="broken" />
			</run>
		</groups>
		<classes>
			<class name="com.managers.TestConfig" />
			<class name="com.tests.HomePageTest" />
		</classes>
	</test> <!-- Test -->
</suite>